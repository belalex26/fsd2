mixin range-dates({titleIn, titleOut, data})
 
  .range-dates
    label.range-dates__title(for="range-dates__start-date")= titleIn
      input.datepicker-here.range-dates__input.range-dates__start-date#range-dates__start-date(type="text", placeholder="ДД.ММ.ГГГГ", data-range="true", data-multiple-dates-separator=" - ",  data-clear-Button="true" , data-apply-Button="true")
    
    label.range-dates__title(for="range-dates__end-date")= titleOut
      input.range-dates__input#range-dates__end-date(type="text", placeholder="19.08.2019")